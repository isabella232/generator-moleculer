{
  "name": "<%=projectName %>",
  "version": "1.0.0",
  "description": "My Moleculer microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner"<% if (jest) { %>,
    "ci": "jest --watch",
    "test": "jest --coverage"<% } %><% if (lint) { %>,
    "lint": "eslint services"<% } %>
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    <% if (lint) { %>
    "eslint": "^4.18.2",
    <% } %>
    <% if (jest) { %>
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    <% } %>
    "moleculer-repl": "^0.4.0"
  },
  "dependencies": {
    <% if (apiGW) { %>
    "moleculer-web": "^0.6.0",
    <% } %>
    <% if (transporter == "NATS") { %>
    "nats": "^0.8.2",
    <% } %>
    <% if (transporter == "MQTT") { %>
    "mqtt": "^2.16.0",
    <% } %>
    <% if (transporter == "AMQP") { %>
    "amqplib": "^0.5.2",
    <% } %>
    <% if (transporter == "STAN") { %>
    "node-nats-streaming": "0.0.28",
    <% } %>
    <% if (transporter == "Kafka") { %>
    "kafka-node": "2.4.1",
    <% } %>
    <% if (redis) { %>
    "ioredis": "^3.2.2",
    <% } %>
    "moleculer": "^0.12.0"
  },
  "engines": {
    "node": ">= 6.x.x"
  }<% if (jest) { %>,
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }<% } %>
}
